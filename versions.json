    },
    "modules": [
        {
            "name": "kilua-bootstrap",
            "description": "Bootstrap",
            "excludes": [
                "kilua-tailwindcss"
            ],
            "initializers": [
                "BootstrapModule",
                "BootstrapCssModule"
            ]
        },
        {
            "name": "kilua-tailwindcss",
            "description": "TailwindCSS",
            "excludes": [
                "kilua-bootstrap"
            ],
            "initializers": [
                "TailwindcssModule"
            ]
        },
        {
            "name": "kilua-fontawesome",
            "description": "Font Awesome Icons",
            "initializers": [
                "FontAwesomeModule"
            ]
        },
        {
            "name": "kilua-bootstrap-icons",
            "description": "Bootstrap Icons",
            "initializers": [
                "BootstrapIconsModule"
            ]
        },
        {
            "name": "kilua-routing",
            "description": "Routing"
        },
        {
            "name": "kilua-i18n",
            "description": "Internationalization"
        },
        {
            "name": "kilua-tempus-dominus",
            "description": "Date and time picker form components",
            "initializers": [
                "TempusDominusModule"
            ]
        },
        {
            "name": "kilua-trix",
            "description": "Rich text form component",
            "initializers": [
                "TrixModule"
            ]
        },
        {
            "name": "kilua-tom-select",
