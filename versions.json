{
    "kilua": "0.0.25",
    "kotlin": "2.2.0-RC",
    "compose": "1.8.1",
    "coroutines": "1.10.2",
    "ksp": "2.2.0-RC-2.0.1",
    "kilua-rpc": "0.0.34",
    "logback": "1.5.18",
    "template-javalin": {
    },
    "template-jooby": {
        "jooby": "3.8.1"
    },
    "template-ktor": {
        "ktor": "3.1.3"
    },
    "template-micronaut": {
        "micronaut": "4.8.2",
